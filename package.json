{
  "name": "i18n-esign",
  "displayName": "i18n-esign",
  "description": "Combine I18n json files to single excel ",
  "version": "0.1.1",
  "publisher": "ybq",
  "repository": {
    "type": "git",
    "url": "https://github.com/yubaoquan/i18n-esign"
  },
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:i18n-esign.excel",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact",
    "onLanguage:javascript",
    "onLanguage:html",
    "onLanguage:vue"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "i18n-esign",
      "properties": {
        "i18n-esign.enableLinter": {
          "type": "boolean",
          "default": true,
          "description": "Mark string literals in editor so you can quickly spot on them"
        },
        "i18n-esign.markColor": {
          "type": "string",
          "default": "red",
          "description": "The color of matched non-English letters"
        }
      }
    },
    "commands": [
      {
        "command": "i18n-esign.excel",
        "title": "%extension.menu.btn%"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "i18n-esign.excel",
          "group": "7_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@angular/compiler": "^9.1.4",
    "@babel/core": "^7.9.6",
    "@types/babel__core": "^7.1.7",
    "@types/fs-extra": "^8.1.0",
    "@types/lodash": "^4.14.150",
    "fs-extra": "^9.0.0",
    "globby": "^11.0.0",
    "json2xls": "^0.1.2",
    "lodash": "^4.17.15",
    "vue-template-compiler": "^2.6.11"
  }
}
